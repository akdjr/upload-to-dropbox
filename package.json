{
  "name": "myxie-upload-to-dropbox",
  "version": "5.0.0",
  "private": true,
  "description": "Upload files to Dropbox action (forked from upload-to-dropbox)",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build --source-map",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts .",
    "lint:prettier": "prettier **/*.{js,ts} --check",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "test": "jest --passWithNoTests",
    "all": "run-s lint test build package",
    "debug": "tsx src/dev.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akdjr/upload-to-dropbox.git"
  },
  "keywords": [
    "actions",
    "dropbox",
    "github-actions"
  ],
  "author": "Akshay Dhalwala <akshay.dhalwala@gmail.com> (forked from Deka <dekachan16@gmail.com>)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "dropbox": "^10.34.0",
    "globby": "^14.0.2",
    "node-fetch": "^3.3.2"
  },
  "packageManager": "pnpm@9.10.0",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^22.10.2",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}